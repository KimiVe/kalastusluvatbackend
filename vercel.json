{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MYSQLHOST": "@mysqlhost",
    "MYSQLUSER": "@mysqluser", 
    "MYSQLPASSWORD": "@mysqlpassword",
    "MYSQLDATABASE": "@mysqldatabase",
    "MYSQLPORT": "@mysqlport"
  }
}